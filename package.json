{
  "name": "stremio-veoveo-search-addon",
  "version": "1.3.0",
  "description": "VeoVeo Search Pro - Advanced search addon for Stremio with cascading search system for movies, series and anime",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "bun run --hot index.js",
    "start": "bun run index.js",
    "start:prod": "NODE_ENV=production bun run index.js",
    "diagnostic": "bun run scripts/diagnostic-server.js",
    "validate-channels": "bun run scripts/validate-channels.js",
    "update-logos": "bun run scripts/update-logos.js",
    "deploy": "./scripts/deploy.sh",
    "test": "vitest",
    "test:unit": "vitest run --reporter=verbose",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:domain": "vitest run tests/unit/domain",
    "test:application": "vitest run tests/unit/application",
    "test:infrastructure": "vitest run tests/unit/infrastructure",
    "lint": "eslint src/ --ext .js",
    "publish-addon": "bun run scripts/publish-addon.js",
    "build": "bun install",
    "docker:build": "bun install && docker build -t stremio-veoveo-addon .",
    "docker:run": "docker run -p 7000:7000 stremio-veoveo-addon",
    "preview": "bun run start:prod",
    "beamup-deploy": "bunx beamup-cli deploy"
  },
  "keywords": [
    "stremio",
    "addon",
    "veoveo",
    "search",
    "magnet",
    "torrent",
    "clean-architecture",
    "ddd"
  ],
  "author": "VeoVeo Search Pro Addon",
  "license": "MIT",
  "dependencies": {
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.1",
    "kitsu": "^11.1.0",
    "knex-stringcase": "^1.4.6",
    "parse-magnet-uri": "^4.0.0",
    "pg": "^8.12.0",
    "socks-proxy-agent": "^8.0.5",
    "stremio-addon-sdk": "^1.6.8",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.6",
    "@vitest/ui": "^4.0.6",
    "eslint": "^8.57.0",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.1.0",
    "vitest": "^4.0.6"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stremio/veoveo-search-addon.git"
  },
  "bugs": {
    "url": "https://github.com/stremio/veoveo-search-addon/issues"
  },
  "homepage": "https://github.com/stremio/veoveo-search-addon#readme"
}
